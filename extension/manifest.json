{
    "name": "BSS B2B support extension",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Redirects specific script requests.",
    "permissions": [
        "storage",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "scripting"
    ],
    "host_permissions": [
        "*://cdn.shopify.com/*",
        "*://*.myshopify.com/*",
        "*://*.shopifypreview.com/*",
        "*://well-daily.com/*",
        "*://mymovr.de/*",
        "*://vendysstore.com/*",
        "*://pinkysmicros.com/*",
        "*://*.gpworkers.com/*",
        "*://drinknativa.com/*",
        "*://*.venyss.com/*",
        "*://naturalhealthworld.co.uk/*",
        "*://uk.ahmadtea.com/*",
        "*://bigbenmedical.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
